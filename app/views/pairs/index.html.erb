<div class="time col-md-offset-4 col-md-4 page-title">
  <h2><%= Time.now.strftime("%A %B %d") %></h2>
</div>
<div class="pairs-wrapper col-md-offset-3 col-md-6">
  <table class="table table-striped">
    <% if @pairs %>
      <% @pairs.each do |pair| %>
        <tr>
          <td>
            <%= pair.student.name %>
          </td>
          <td>
            <%= pair.partner.name %>
          </td>
        </tr>
      <%end%>
    <% end %>
  </div>
</div
